{
    "name": "rosiefaulkner/markdown-to-html",
    "description": "Markdown parser in PHP",
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "require":{
        "php": "8.0.14"
    },
    "autoload": {
        "psr-4": {
            "Rosiefaulkner\\MarkdownToHtml\\": "src/"
        }
    },
    "post-install-cmd": [
        "php artisan clear-compiled",
        "chmod -R 777 src/"
    ],
    "authors": [
        {
            "name": "Rosie Faulkner",
            "email": "faulknerproject@gmail.com"
        }
    ]
}
