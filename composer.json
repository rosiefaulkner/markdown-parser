{
    "name": "rosiefaulkner/markdown-to-html",
    "description": "Markdown parser in PHP",
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpunit/php-code-coverage": "^9.2.3"
    },
    "require":{
        "php": "^7.2.5 || ^8"
    },
    "autoload": {
        "psr-4": {
            "Rosiefaulkner\\MarkdownToHtml\\": "src/"
        }
    },
    "post-install-cmd": [
        "php artisan clear-compiled",
        "chmod -R 777 src/"
    ],
    "authors": [
        {
            "name": "Rosie Faulkner",
            "email": "faulknerproject@gmail.com"
        }
    ]
}
